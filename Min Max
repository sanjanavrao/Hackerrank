def miniMaxSum(arr):
    total=sum(arr)
    sol=[]
    for i in range(len(arr)):
        value=total-arr[i]
        sol.append(value)
    minimum=min(sol)
    maximum=max(sol)
    print(minimum,maximum)
        
    

if __name__ == '__main__':

    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
